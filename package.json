{
  "name": "bigred-link",
  "version": "1.1.0",
  "description": "URL Shortener",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">= 16.x",
    "npm": ">= 8.x"
  },
  "scripts": {
    "build": "vite build",
    "clean": "rm -rf ./build",
    "dev": "vite serve --port $PORT",
    "prebuild": "npm run clean",
    "preview": "vite preview",
    "start": "vercel dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": {
    "name": "Michael Sprague",
    "email": "mikesprague@gmail.com"
  },
  "homepage": "https://bigred.link",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/mikesprague/bigred-link"
  },
  "bugs": {
    "url": "https://github.com/mikesprague/bigred-link/issues"
  },
  "dependencies": {
    "@bugsnag/core": "7.17.4",
    "@bugsnag/js": "7.17.4",
    "@bugsnag/plugin-react": "7.17.4",
    "@fortawesome/fontawesome-svg-core": "6.2.0",
    "@fortawesome/free-brands-svg-icons": "6.2.0",
    "@fortawesome/free-regular-svg-icons": "6.2.0",
    "@fortawesome/free-solid-svg-icons": "6.2.0",
    "@fortawesome/react-fontawesome": "0.2.0",
    "axios": "0.27.2",
    "clipboard-polyfill": "3.0.3",
    "dayjs": "1.11.5",
    "dompurify": "2.4.0",
    "dotenv": "16.0.2",
    "mongodb": "4.10.0",
    "nanoid": "4.0.0",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.19.1",
    "@babel/eslint-plugin": "7.19.1",
    "@babel/preset-env": "7.19.1",
    "@fullhuman/postcss-purgecss": "5.0.0",
    "@tailwindcss/typography": "0.5.7",
    "@tailwindcss/forms": "0.5.3",
    "@vitejs/plugin-react": "2.1.0",
    "autoprefixer": "10.4.12",
    "babel-plugin-add-react-displayname": "0.0.5",
    "cssnano": "5.1.13",
    "eslint": "8.23.1",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-stylelint": "16.0.0",
    "eslint-plugin-compat": "4.0.2",
    "eslint-plugin-html": "7.1.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-markdown": "3.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-security": "1.5.0",
    "postcss": "8.4.16",
    "postcss-cli": "10.0.0",
    "prettier": "2.7.1",
    "prettier-eslint": "15.0.1",
    "prettier-stylelint": "0.4.2",
    "sass": "1.54.9",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-standard": "28.0.0",
    "stylelint-config-standard-scss": "5.0.0",
    "stylelint-prettier": "2.0.0",
    "tailwindcss": "3.1.8",
    "vercel": "28.4.0",
    "vite": "3.1.3",
    "vite-plugin-html": "3.2.0",
    "vite-plugin-pwa": "0.13.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "settings": {
      "jest": {
        "version": 28
      }
    },
    "extends": [
      "airbnb",
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "plugin:jsx-a11y/recommended",
      "prettier",
      "stylelint"
    ],
    "plugins": [
      "compat",
      "html",
      "import",
      "jsx-a11y",
      "markdown",
      "prettier",
      "react",
      "react-hooks"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "allowImportExportEverywhere": true,
      "parser": "@eslint/babel-parser",
      "ecmaFeatures": {
        "jsx": true,
        "impliedStrict": true,
        "modules": true
      },
      "sourceType": "module"
    },
    "rules": {
      "react/jsx-filename-extension": [
        "error",
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/function-component-definition": [
        2,
        {
          "namedComponents": "arrow-function",
          "unnamedComponents": "arrow-function"
        }
      ],
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "error",
      "react/jsx-boolean-value": "off",
      "jest/no-deprecated-functions": "off",
      "jsx-a11y/no-noninteractive-element-interactions": "off",
      "jsx-a11y/no-static-element-interactions": "off",
      "jsx-a11y/click-events-have-key-events": "off",
      "jsx-a11y/no-onchange": "off",
      "import/prefer-default-export": "off",
      "no-console": "warn",
      "node/no-unpublished-import": "warn",
      "node/no-missing-import": "warn",
      "import/extensions": "warn"
    }
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  }
}

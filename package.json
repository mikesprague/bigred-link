{
  "name": "bigred-link",
  "version": "0.1.0",
  "description": "URL Shortener",
  "private": true,
  "engines": {
    "node": ">= 14.x",
    "npm": ">= 7.x",
    "yarn": ">= 1.22.x"
  },
  "scripts": {
    "build": "rm -rf ./public && webpack --color",
    "prepublish": "yarn snyk-protect",
    "snyk-protect": "snyk protect",
    "start": "cross-env NODE_ENV=development vercel dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": {
    "name": "Michael Sprague",
    "email": "mikesprague@gmail.com"
  },
  "homepage": "https://bigred.link",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/mikesprague/bigred-link"
  },
  "bugs": {
    "url": "https://github.com/mikesprague/bigred-link/issues"
  },
  "dependencies": {
    "@bugsnag/core": "^7.14.0",
    "@bugsnag/js": "^7.14.1",
    "@bugsnag/plugin-react": "^7.14.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/pro-duotone-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "axios": "^0.24.0",
    "clipboard-polyfill": "^3.0.3",
    "dayjs": "^1.10.7",
    "dompurify": "^2.3.4",
    "dotenv": "^11.0.0",
    "he": "^1.2.0",
    "logrocket": "^2.1.3",
    "logrocket-react": "^4.0.1",
    "mongodb": "^4.3.0",
    "nanoid": "^3.1.32",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "register-service-worker": "^1.7.2",
    "snyk": "^1.826.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/eslint-plugin": "^7.16.5",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-export-default-from": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-flow": "^7.16.7",
    "@babel/plugin-transform-object-assign": "^7.16.7",
    "@babel/plugin-transform-react-jsx": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.8",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-react": "^7.16.7",
    "@babel/runtime": "^7.16.7",
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@octokit/core": "^3.5.1",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "babel-plugin-add-react-displayname": "^0.0.5",
    "compression-webpack-plugin": "^9.2.0",
    "concurrently": "^7.0.0",
    "copy-webpack-plugin": "^10.2.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.3.1",
    "cssnano": "^5.0.15",
    "dotenv": "^11.0.0",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-compat": "^4.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-security": "^1.4.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.4.7",
    "node-sass": "^7.0.1",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "prettier-stylelint": "^0.4.2",
    "sass-lint": "^1.13.1",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.0.13",
    "terser-webpack-plugin": "^5.3.0",
    "typescript": "^4.5.4",
    "vercel": "^23.1.2",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1",
    "workbox-webpack-plugin": "^6.4.2"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-export-default-from",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-transform-object-assign",
      "@babel/plugin-transform-runtime",
      "add-react-displayname"
    ],
    "presets": [
      "@babel/preset-env",
      "@babel/react"
    ]
  },
  "browserslist": {
    "development": [
      "last 1 version"
    ],
    "production": [
      "> 2%",
      "last 2 versions",
      "not ie <= 10"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "airbnb",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "plugin:jsx-a11y/recommended",
      "prettier"
    ],
    "plugins": [
      "compat",
      "html",
      "import",
      "jsx-a11y",
      "markdown",
      "prettier",
      "react",
      "react-hooks",
      "security"
    ],
    "parserOptions": {
      "ecmaVersion": 11,
      "allowImportExportEverywhere": true,
      "parser": "babel-parser",
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module"
    },
    "rules": {
      "react/jsx-filename-extension": [
        "error",
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn",
      "consistent-return": "warn"
    }
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "tabwidth": true,
    "trailingComma": "all"
  }
}

{
  "name": "bigred-link",
  "version": "1.5.15",
  "description": "A big red URL shortener",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">= 18.x",
    "npm": ">= 8.x"
  },
  "scripts": {
    "back-up-db": "node ./src/utils/back-up-db.js",
    "build": "vite build",
    "check-all-urls": "node ./src/utils/safe-browsing-url-check.js",
    "clean": "rm -rf ./build",
    "dev": "vite serve --port $PORT",
    "prebuild": "npm run clean",
    "preview": "vite preview",
    "start": "vercel dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": {
    "name": "Michael Sprague",
    "email": "mikesprague@gmail.com"
  },
  "homepage": "https://bigred.link",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/mikesprague/bigred-link"
  },
  "bugs": {
    "url": "https://github.com/mikesprague/bigred-link/issues"
  },
  "dependencies": {
    "@bugsnag/core": "7.19.0",
    "@bugsnag/js": "7.22.4",
    "@bugsnag/plugin-react": "7.19.0",
    "@fortawesome/fontawesome-svg-core": "6.5.1",
    "@fortawesome/free-brands-svg-icons": "6.5.1",
    "@fortawesome/free-regular-svg-icons": "6.5.1",
    "@fortawesome/free-solid-svg-icons": "6.5.1",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@supabase/supabase-js": "2.39.7",
    "axios": "1.6.7",
    "clipboard-polyfill": "4.0.2",
    "dayjs": "1.11.10",
    "dompurify": "3.0.9",
    "dotenv": "16.4.5",
    "nanoid": "5.0.6",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.24.0",
    "@babel/preset-react": "7.23.3",
    "@biomejs/biome": "1.5.3",
    "@fullhuman/postcss-purgecss": "5.0.0",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.10",
    "@vitejs/plugin-react-swc": "3.6.0",
    "autoprefixer": "10.4.18",
    "babel-plugin-add-react-displayname": "0.0.5",
    "cssnano": "6.1.0",
    "cz-git": "^1.9.0",
    "postcss": "8.4.35",
    "postcss-cli": "11.0.0",
    "sass": "1.71.1",
    "stylelint": "16.2.1",
    "stylelint-config-standard": "36.0.0",
    "tailwindcss": "3.4.1",
    "vercel": "33.5.4",
    "vite": "5.1.5",
    "vite-plugin-html": "3.2.2",
    "vite-plugin-pwa": "0.19.2"
  },
  "babel": {
    "presets": ["@babel/preset-env", "@babel/preset-react"]
  },
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all"],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  }
}

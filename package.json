{
  "name": "bigred.link",
  "version": "0.0.1",
  "description": "URL Shortener",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": ">= 12.x",
    "npm": ">= 6.x",
    "yarn": ">= 1.x"
  },
  "scripts": {
    "assets": "webpack --color --display-max-modules 0",
    "assets:watch": "yarn assets -d -w --color true --display minimal",
    "build": "cross-env NODE_ENV=production yarn assets -p",
    "build:dev": "yarn clean && cross-env NODE_ENV=development yarn assets -d",
    "clean": "rm -rf public",
    "dev": "concurrently 'yarn serve' 'yarn assets:watch' --names 'ðŸ–¥ ,ðŸ“¦ ' --prefix '[{name}] '",
    "prebuild": "yarn clean",
    "prepublish": "yarn snyk-protect",
    "snyk-protect": "snyk protect",
    "serve": "cross-env NODE_ENV=development nodemon ./index.js --ignore public/",
    "serve:debug": "cross-env NODE_ENV=development nodemon ./index.js --inspect --ignore public/",
    "start": "node ./index.js",
    "start:debug": "cross-env NODE_ENV=productiom node ./index.js --inspect",
    "test": "jest",
    "test:snyk": "snyk test"
  },
  "repository": {
    "url": "https://github.com/mikesprague/bigred-link"
  },
  "author": {
    "name": "Michael Sprague",
    "email": "mikesprague@gmail.com"
  },
  "keywords": [
    "url shortener",
    "nodejs",
    "express",
    "mongodb"
  ],
  "license": "MIT",
  "homepage": "https://bigred.link",
  "bugs": {
    "url": "https://github.com/mikesprague/bigred-link/issues"
  },
  "dependencies": {
    "@bugsnag/js": "^7.0.0",
    "@bugsnag/plugin-express": "^7.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/pro-duotone-svg-icons": "^5.13.0",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.4.1",
    "clipboard-polyfill": "^2.8.6",
    "cross-env": "^7.0.2",
    "dompurify": "^2.0.8",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "logrocket": "^1.0.7",
    "mongodb": "^3.5.6",
    "nanoid": "^3.1.3",
    "snyk": "^1.307.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/node": "7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-object-assign": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/runtime": "^7.9.2",
    "@fullhuman/postcss-purgecss": "^2.1.2",
    "autoprefixer": "^9.7.6",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.4.0",
    "babel-loader": "^8.1.0",
    "compression-webpack-plugin": "^3.1.0",
    "concurrently": "^5.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.2",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-compat": "^3.5.1",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-security": "^1.4.0",
    "html-webpack-plugin": "^4.2.0",
    "jest": "^25.4.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.3",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.4",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.4",
    "terser-webpack-plugin": "^2.3.5",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpackbar": "^4.0.0"
  },
  "browserslist": {
    "development": [
      "last 1 version"
    ],
    "production": [
      "> 1%",
      "last 2 versions",
      "not ie <= 10"
    ]
  },
  "babel": {
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-transform-object-assign"
    ],
    "presets": [
      "@babel/preset-env"
    ]
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 10,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true,
        "modules": true
      }
    },
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "airbnb-base"
    ],
    "plugins": [
      "compat",
      "import",
      "html",
      "markdown",
      "prettier",
      "security"
    ],
    "rules": {
      "func-names": "off",
      "no-console": "warn",
      "no-unused-vars": [
        "warn",
        {
          "argsIgnorePattern": "res|next|^err"
        }
      ],
      "max-len": "off",
      "no-restricted-globals": "warn",
      "no-underscore-dangle": "off",
      "space-before-function-paren": "off"
    }
  },
  "prettier": {
    "arrowParens": "avoid",
    "plugins": [
      "all"
    ],
    "printWidth": 120,
    "proseWrap": "always",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  },
  "snyk": true
}

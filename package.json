{
  "name": "bigred-link",
  "version": "1.6.2",
  "description": "A big red URL shortener",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">= 20.x",
    "npm": ">= 10.x"
  },
  "scripts": {
    "back-up-db": "node ./src/utils/back-up-db.js",
    "build": "vite build",
    "check-all-urls": "node ./src/utils/safe-browsing-url-check.js",
    "clean": "rm -rf ./build",
    "dev": "vite serve --port $PORT",
    "prebuild": "npm run clean",
    "preview": "vite preview",
    "start": "vercel dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": {
    "name": "Michael Sprague",
    "email": "mikesprague@gmail.com"
  },
  "homepage": "https://bigred.link",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/mikesprague/bigred-link"
  },
  "bugs": {
    "url": "https://github.com/mikesprague/bigred-link/issues"
  },
  "dependencies": {
    "@bugsnag/core": "8.1.1",
    "@bugsnag/js": "8.1.2",
    "@bugsnag/plugin-react": "8.1.1",
    "@fortawesome/fontawesome-svg-core": "6.7.1",
    "@fortawesome/free-brands-svg-icons": "6.7.1",
    "@fortawesome/free-regular-svg-icons": "6.7.1",
    "@fortawesome/free-solid-svg-icons": "6.7.1",
    "@fortawesome/react-fontawesome": "0.2.2",
    "@supabase/supabase-js": "2.46.2",
    "axios": "1.7.9",
    "clipboard-polyfill": "4.1.0",
    "dayjs": "1.11.13",
    "dompurify": "3.2.2",
    "dotenv": "16.4.6",
    "jotai": "2.10.3",
    "nanoid": "5.0.9",
    "prop-types": "15.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@fullhuman/postcss-purgecss": "7.0.2",
    "@tailwindcss/forms": "0.5.9",
    "@tailwindcss/typography": "0.5.15",
    "@vitejs/plugin-react-swc": "3.7.2",
    "autoprefixer": "10.4.20",
    "cssnano": "7.0.6",
    "cz-git": "^1.11.0",
    "postcss": "8.4.49",
    "postcss-cli": "11.0.0",
    "sass": "1.81.1",
    "stylelint": "16.11.0",
    "stylelint-config-standard": "36.0.1",
    "tailwindcss": "3.4.15",
    "vercel": "39.1.2",
    "vite": "6.0.2",
    "vite-plugin-html": "3.2.2",
    "vite-plugin-pwa": "0.21.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "volta": {
    "node": "22.4.1",
    "npm": "10.8.1"
  }
}

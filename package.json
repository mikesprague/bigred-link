{
  "name": "bigred-link",
  "version": "0.1.0",
  "description": "URL Shortener",
  "private": true,
  "engines": {
    "node": ">= 16.x",
    "npm": ">= 8.x"
  },
  "scripts": {
    "build": "rm -rf ./public && webpack --color",
    "start": "cross-env NODE_ENV=development vercel dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": {
    "name": "Michael Sprague",
    "email": "mikesprague@gmail.com"
  },
  "homepage": "https://bigred.link",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/mikesprague/bigred-link"
  },
  "bugs": {
    "url": "https://github.com/mikesprague/bigred-link/issues"
  },
  "dependencies": {
    "@bugsnag/core": "7.17.4",
    "@bugsnag/js": "7.17.4",
    "@bugsnag/plugin-react": "7.17.4",
    "@fortawesome/fontawesome-svg-core": "6.2.0",
    "@fortawesome/free-brands-svg-icons": "6.2.0",
    "@fortawesome/pro-duotone-svg-icons": "6.2.0",
    "@fortawesome/react-fontawesome": "0.2.0",
    "axios": "0.27.2",
    "clipboard-polyfill": "3.0.3",
    "dayjs": "1.11.5",
    "dompurify": "2.4.0",
    "dotenv": "16.0.2",
    "he": "1.2.0",
    "logrocket": "3.0.1",
    "logrocket-react": "5.0.1",
    "mongodb": "4.9.1",
    "nanoid": "3.3.4",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "register-service-worker": "1.7.2"
  },
  "devDependencies": {
    "@babel/core": "7.19.0",
    "@babel/eslint-parser": "7.18.9",
    "@babel/eslint-plugin": "7.18.10",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-export-default-from": "7.18.10",
    "@babel/plugin-proposal-object-rest-spread": "7.18.9",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-syntax-flow": "7.18.6",
    "@babel/plugin-transform-object-assign": "7.18.6",
    "@babel/plugin-transform-react-jsx": "7.19.0",
    "@babel/plugin-transform-runtime": "7.18.10",
    "@babel/preset-env": "7.19.0",
    "@babel/preset-react": "7.18.6",
    "@babel/runtime": "7.19.0",
    "@fullhuman/postcss-purgecss": "5.0.0",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/typography": "0.5.7",
    "autoprefixer": "10.4.10",
    "babel-loader": "8.2.5",
    "babel-plugin-add-react-displayname": "0.0.5",
    "compression-webpack-plugin": "10.0.0",
    "concurrently": "7.4.0",
    "copy-webpack-plugin": "11.0.0",
    "cross-env": "7.0.3",
    "css-loader": "6.7.1",
    "css-minimizer-webpack-plugin": "4.1.0",
    "cssnano": "5.1.13",
    "dotenv": "16.0.2",
    "eslint": "8.23.1",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-stylelint": "16.0.0",
    "eslint-plugin-compat": "4.0.2",
    "eslint-plugin-html": "7.1.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-markdown": "3.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-security": "1.5.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.0",
    "mini-css-extract-plugin": "2.6.1",
    "postcss": "8.4.16",
    "postcss-cli": "10.0.0",
    "postcss-loader": "7.0.1",
    "prettier": "2.7.1",
    "prettier-eslint": "15.0.1",
    "prettier-stylelint": "0.4.2",
    "sass": "1.54.9",
    "sass-loader": "13.0.2",
    "style-loader": "3.3.1",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-standard": "28.0.0",
    "stylelint-config-standard-scss": "5.0.0",
    "stylelint-prettier": "2.0.0",
    "tailwindcss": "3.1.8",
    "terser-webpack-plugin": "5.3.6",
    "vercel": "28.2.5",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "workbox-webpack-plugin": "6.5.4"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-export-default-from",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-transform-object-assign",
      "@babel/plugin-transform-runtime",
      "add-react-displayname"
    ],
    "presets": [
      "@babel/preset-env",
      "@babel/react"
    ]
  },
  "browserslist": {
    "development": [
      "last 1 version"
    ],
    "production": [
      "> 2%",
      "last 2 versions",
      "not ie <= 10"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "airbnb",
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "plugin:jsx-a11y/recommended",
      "prettier",
      "stylelint"
    ],
    "plugins": [
      "compat",
      "html",
      "import",
      "jsx-a11y",
      "markdown",
      "prettier",
      "react",
      "react-hooks"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser",
      "ecmaVersion": 11,
      "allowImportExportEverywhere": true,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true,
        "modules": true,
        "jsx": true
      }
    },
    "rules": {
      "react/jsx-filename-extension": [
        "error",
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn",
      "consistent-return": "warn"
    }
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "tabwidth": true,
    "trailingComma": "all"
  }
}

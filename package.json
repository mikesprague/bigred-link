{
  "name": "bigred-link",
  "version": "1.6.0",
  "description": "A big red URL shortener",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">= 20.x",
    "npm": ">= 10.x"
  },
  "scripts": {
    "back-up-db": "node ./src/utils/back-up-db.js",
    "build": "vite build",
    "check-all-urls": "node ./src/utils/safe-browsing-url-check.js",
    "clean": "rm -rf ./build",
    "dev": "vite serve --port $PORT",
    "prebuild": "npm run clean",
    "preview": "vite preview",
    "start": "vercel dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": {
    "name": "Michael Sprague",
    "email": "mikesprague@gmail.com"
  },
  "homepage": "https://bigred.link",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/mikesprague/bigred-link"
  },
  "bugs": {
    "url": "https://github.com/mikesprague/bigred-link/issues"
  },
  "dependencies": {
    "@bugsnag/core": "7.22.7",
    "@bugsnag/js": "7.23.0",
    "@bugsnag/plugin-react": "7.24.0",
    "@fortawesome/fontawesome-svg-core": "6.5.2",
    "@fortawesome/free-brands-svg-icons": "6.5.2",
    "@fortawesome/free-regular-svg-icons": "6.5.2",
    "@fortawesome/free-solid-svg-icons": "6.5.2",
    "@fortawesome/react-fontawesome": "0.2.2",
    "@supabase/supabase-js": "2.44.0",
    "axios": "1.7.2",
    "clipboard-polyfill": "4.1.0",
    "dayjs": "1.11.11",
    "dompurify": "3.1.5",
    "dotenv": "16.4.5",
    "jotai": "2.8.3",
    "nanoid": "5.0.7",
    "prop-types": "15.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@fullhuman/postcss-purgecss": "6.0.0",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.13",
    "@vitejs/plugin-react-swc": "3.7.0",
    "autoprefixer": "10.4.19",
    "cssnano": "7.0.3",
    "cz-git": "^1.9.3",
    "postcss": "8.4.38",
    "postcss-cli": "11.0.0",
    "sass": "1.77.6",
    "stylelint": "16.6.1",
    "stylelint-config-standard": "36.0.1",
    "tailwindcss": "3.4.4",
    "vercel": "34.2.8",
    "vite": "5.3.1",
    "vite-plugin-html": "3.2.2",
    "vite-plugin-pwa": "0.20.0"
  },
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all"],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "volta": {
    "node": "22.2.0",
    "npm": "10.8.0"
  }
}
